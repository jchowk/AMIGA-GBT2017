	Doi = {10.1111/j.1365-2966.2011.18896.x},
	Eprint = {1102.3912},
	Journal = {\mnras},
	Keywords = {stars: formation, galaxies: evolution, galaxies: formation, intergalactic medium, cosmology: theory},
	Month = aug,
	Pages = {2782-2789},
	Title = {{The drop in the cosmic star formation rate below redshift 2 is caused by a change in the mode of gas accretion and by active galactic nucleus feedback}},
	Volume = 415,
	Year = 2011,
