	Keywords = {Atlases, Galaxies: Individual: Name: Large Magellanic Cloud, Galaxies: Individual: Name: Small Magellanic Cloud, Galaxy: Halo, ISM: Structure, Galaxies: Magellanic Clouds, Ultraviolet: ISM},
	Month = mar,
	Pages = {81-189},
	Title = {{An Atlas of Far Ultraviolet Spectroscopic Explorer Sight Lines toward the Magellanic Clouds}},
