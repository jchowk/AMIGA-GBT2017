	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1501.03155},
	Author = {{Muratov}, A.~L. and {Kere{\v s}}, D. and {Faucher-Gigu{\`e}re}, C.-A. and {Hopkins}, P.~F. and {Quataert}, E. and {Murray}, N.},
	Date-Added = {2016-02-19 16:04:53 +0000},
	Date-Modified = {2016-02-19 16:04:54 +0000},
	Doi = {10.1093/mnras/stv2126},
	Eprint = {1501.03155},
	Journal = {\mnras},
	Keywords = {stars: formation, galaxies: evolution, galaxies: formation, cosmology: theory},
	Month = dec,
	Pages = {2691-2713},
	Title = {{Gusty, gaseous flows of FIRE: galactic winds in cosmological simulations with explicit stellar feedback}},
	Volume = 454,
	Year = 2015,
	Bdsk-File-1 = {},
