	Journal = {\apj},
	Keywords = {dust, extinction, galaxies: star clusters: general, intergalactic medium, ISM: structure, quasars: absorption lines},
	Month = jul,
	Pages = {66},
	Primaryclass = {astro-ph.CO},
	Title = {{Radiation Pressure from Massive Star Clusters as a Launching Mechanism for Super-galactic Winds}},
	Volume = 735,
	Year = 2011,
