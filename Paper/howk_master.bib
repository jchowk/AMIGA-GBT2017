	Journal = {\apjs},
	Keywords = {Cosmology: Observations, Galaxies: Intergalactic Medium, Galaxies: Quasars: Absorption Lines},
	Month = nov,
	Pages = {37-70},
	Title = {{A Space Telescope Imaging Spectrograph Survey for O VI Absorption Systems at 0.12 $\lt$ z $\lt$\~{} 0.5. II. Physical Conditions of the Ionized Gas}},
	Volume = 179,
	Year = 2008,
	Bdsk-File-1 = {},
