	Author = {{Brandt}, J.~C. and {Heap}, S.~R. and {Beaver}, E.~A. and {Boggess}, A. and {Carpenter}, K.~G. and {Ebbets}, D.~C. and {Hutchings}, J.~B. and {Jura}, M. and {Leckrone}, D.~S. and {Linsky}, J.~L. and {Maran}, S.~P. and {Savage}, B.~D. and {Smith}, A.~M. and {Trafton}, L.~M. and {Walter}, F.~M. and {Weymann}, R.~J. and {Howk}, J.~C. and {Snow}, M. and {Ake}, T.~B. and {Sembach}, K.~R.},
