	Date-Modified = {2016-03-02 14:35:31 +0000},
	Doi = {10.1093/mnras/stw178},
	Eprint = {1601.05090},
	Journal = {\mnras},
	Keywords = {ISM: jets and outflows, galaxies: evolution, galaxies: formation, ultraviolet: ISM},
	Month = apr,
	Pages = {3133-3161},
	Title = {{Shining a light on galactic outflows: photoionized outflows}},
