	Doi = {10.1086/340231},
	Journal = {\apj},
	Keywords = {ISM: Atoms, ISM: Structure, Ultraviolet: ISM},
	Month = jun,
	Pages = {264-275},
	Title = {{Far-Ultraviolet Spectroscopic Explorer Observations of Degree-Scale Variations in Galactic Halo OVI}},
