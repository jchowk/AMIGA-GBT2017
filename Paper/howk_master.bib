	Doi = {10.1086/301210},
	Eprint = {astro-ph/9910248},
	Journal = {\aj},
	Keywords = {ISM: DUST, EXTINCTION, GALAXIES: INDIVIDUAL (NGC 891), GALAXIES: ISM, GALAXIES: SPIRAL, GALAXIES: STRUCTURE, ISM: CLOUDS},
	Month = feb,
	Pages = {644-667},
	Title = {{The Multiphase Halo of NGC 891: WIYN H{$\alpha$} and BVI Imaging}},
