	Arxivurl = {http://arXiv.org/abs/astro-ph/0303586},
	Author = {{Andr{\'e}}, M.~K. and {Oliveira}, C.~M. and {Howk}, J.~C. and {Ferlet}, R. and {D{\'e}sert}, J.-M. and {H{\'e}brard}, G. and {Lacour}, S. and {Lecavelier des {\'E}tangs}, A. and {Vidal-Madjar}, A. and {Moos}, H.~W.},
	Date-Added = {2015-11-20 15:00:22 +0000},
