	Journal = {\apjl},
	Keywords = {GALAXIES: ABUNDANCES, GALAXIES: INTERGALACTIC MEDIUM, GALAXIES: QUASARS: ABSORPTION LINES, RADIATIVE TRANSFER, Galaxies: Abundances, Galaxies: Intergalactic Medium, Galaxies: Quasars: Absorption Lines, Radiative Transfer},
