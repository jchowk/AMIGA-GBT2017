	Author = {{Heckman}, T.~M. and {Borthakur}, S. and {Overzier}, R. and {Kauffmann}, G. and {Basu-Zych}, A. and {Leitherer}, C. and {Sembach}, K. and {Martin}, D.~C. and {Rich}, R.~M. and {Schiminovich}, D. and {Seibert}, M.},
	Date-Added = {2015-11-20 14:18:12 +0000},
	Date-Modified = {2015-11-20 15:59:05 +0000},
