	Date-Modified = {2017-02-17 01:42:51 +0000},
	Doi = {10.1086/504800},
	Eprint = {astro-ph/0604091},
	Journal = {\apjs},
	Keywords = {Galaxy: Halo, Galaxies: Intergalactic Medium, ISM: Clouds, ISM: Kinematics and Dynamics, Ultraviolet: ISM},
	Month = jul,
	Pages = {229-255},
	Title = {{A Survey of O VI, C III, and H I in Highly Ionized High-Velocity Clouds}},
