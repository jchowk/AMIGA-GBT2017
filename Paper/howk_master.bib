	Author = {{Hafen}, Z. and {Faucher-Giguere}, C.-A. and {Angles-Alcazar}, D. and {Keres}, D. and {Feldmann}, R. and {Chan}, T.~K. and {Quataert}, E. and {Murray}, N. and {Hopkins}, P.~F.},
	Date-Added = {2016-11-15 12:15:23 +0000},
