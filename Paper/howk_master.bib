@phdthesis{rumstay1984,
	Abstract = {To examine whether a statistically viable relation between nebular luminosity and excitation parameter (U) exists, a sample of evolved galactic H II regions was selected for which the exciting stars have been identified. A 10 cm f/8 telescope with a speciality constructed photometer was used to obtain integrated H alpha and H beta fluxes for thirteen Sharpless H II regions. Combining these with previously measured fluxes yielded H alpha fluxes for 27 nebulae, and H beta fluxes for all but three of these. The results indicate that the uncertainty in the value of U calculated from radio continuum flux densities is about 20%. Using H alpha and H beta fluxes and a standard reddening law to correct for interstellar extinction results in an uncertainty of about 25 to 30% in U. In addition these random uncertainties, U is systematically underestimated by 20 to 25%. These results are applied to the spiral galaxy M83. The giant regions more concentrated in a two-armed spiral pattern than the nongiant regions, and have a more sharply peaked radial distribution function.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1984PhDT.........2R},
	Author = {{Rumstay}, K.~S.},
