	Date-Modified = {2016-11-06 22:29:57 +0000},
	Doi = {10.1093/mnras/stw2539},
	Eprint = {1602.06956},
	Journal = {\mnras},
	Month = oct,
	Title = {{NIHAO VIII: Circum-galactic medium and outflows - The puzzles of HI and OVI gas distributions}},
