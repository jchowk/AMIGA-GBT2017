	Arxivurl = {http://arXiv.org/abs/astro-ph/0201353},
	Author = {{Steidel}, C.~C. and {Kollmeier}, J.~A. and {Shapley}, A.~E. and {Churchill}, C.~W. and {Dickinson}, M. and {Pettini}, M.},
	Date-Added = {2015-11-20 14:12:43 +0000},
