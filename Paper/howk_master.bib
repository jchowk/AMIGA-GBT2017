	Eprint = {astro-ph/0501032},
	Journal = {\apjl},
	Keywords = {ISM: Abundances},
	Month = feb,
	Pages = {L39-L42},
	Title = {{Evidence of Correlated Titanium and Deuterium Depletion in the Galactic Interstellar Medium}},
	Volume = 620,
