	Doi = {10.1016/j.ascom.2016.03.002},
	Eprint = {1505.06213},
	Journal = {Astronomy and Computing},
	Keywords = {Galaxy, Abundances-ISM, HII regions-supernovae, General},
	Month = jul,
	Pages = {54-66},
