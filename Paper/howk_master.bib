	Arxivurl = {http://arXiv.org/abs/astro-ph/0604334},
	Author = {{Jorgenson}, R.~A. and {Wolfe}, A.~M. and {Prochaska}, J.~X. and {Lu}, L. and {Howk}, J.~C. and {Cooke}, J. and {Gawiser}, E. and {Gelino}, D.~M.},
