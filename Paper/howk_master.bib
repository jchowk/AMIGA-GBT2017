@book{de-vaucouleurs1991,
	Abstract = {This new, much enlarged catalogue has been made necessary by the explosive growth of extragalactic astronomy over the last 15 years. With data on more than 23,000 galaxies, it includes all galaxies with apparent diameters larger than one arc minute, magnitudes brighter than about magnitude 15.5, and redshifts not larger than 15,000 km/sec, as well as many other objects of interest. Vol. I contains the explanations and references, Vol. II and III contain the catalogue proper. The catalogue gives for each galaxy its position, names, type and luminosity class, optical diameter and axis ratio, position angle, optical, infrared, and neutral hydrogen magnitudes, optical color indices and surface brightnesses, neutral hydrogen indices and line widths, and radial velocities.},
	Adscomment = {Price US$ 198.00. ISBN 3-540-97552-7, Price DM 448.00. ISBN 0-387-97549-7 (Vol. I), ISBN 0-387-97550-0 (Vol. II), ISBN 0-387-97551-9 (Vol. III).},
