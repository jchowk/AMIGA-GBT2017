	Author = {{Faucher-Gigu{\`e}re}, C.-A. and {Hopkins}, P.~F. and {Kere{\v s}}, D. and {Muratov}, A.~L. and {Quataert}, E. and {Murray}, N.},
	Date-Added = {2016-02-24 14:46:37 +0000},
