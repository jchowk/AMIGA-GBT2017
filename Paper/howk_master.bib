	Author = {{Bianco}, F.~B. and {Modjaz}, M. and {Oh}, S.~M. and {Fierroz}, D. and {Liu}, Y.~Q. and {Kewley}, L. and {Graur}, O.},
	Date-Added = {2016-10-19 11:10:19 +0000},
