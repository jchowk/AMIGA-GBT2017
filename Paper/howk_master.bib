	Date-Added = {2015-11-23 14:55:58 +0000},
	Date-Modified = {2015-11-23 14:55:59 +0000},
	Eprint = {astro-ph/9901068},
	Journal = {\aap},
	Keywords = {GALAXIES: INDIVIDUAL: NGC 4013, GALAXIES: ISM, GALAXIES: KINEMATICS AND DYNAMICS, GALAXIES: SPIRAL},
